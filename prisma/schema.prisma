generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id    Int     @default(autoincrement()) @id
  user_name String
  password  String
  address String
  age String
  department_id  String
  point  Int?
}
model departments {
  department_id String @unique
  department_name  String
}
model log {
  id    Int     @default(autoincrement()) @id
  log String
  dt  DateTime @db.DateTime(0)
}