generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id    Int     @default(autoincrement()) @id
  user_name String
  password  String
  department_id  String
}
model departments {
  department_id String @unique
  department_name  String
}